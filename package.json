{
  "name": "timelock-sdk",
  "version": "0.0.180",
  "author": "",
  "main": "./dist/package.cjs",
  "module": "./dist/package.js",
  "devDependencies": {
    "@graphql-codegen/cli": "^6.0.2",
    "@graphql-codegen/typescript": "^5.0.4",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-operations": "^5.0.4",
    "@types/big.js": "^6.2.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "gts": "^6.0.2",
    "tsdown": "^0.16.5",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "zod": "^4.1.12",
    "@uniswap/v3-sdk": "^3.26.0",
    "jsbi": "^4.3.2",
    "big.js": "^7.0.1",
    "@tanstack/react-query": "^5.90.10",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "viem": "^2.39.3",
    "wagmi": "^3.0.1",
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.4",
    "graphql-tag": "^2.12.6"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/package.d.ts",
        "default": "./dist/package.js"
      },
      "require": {
        "types": "./dist/package.d.cts",
        "default": "./dist/package.cjs"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/client.d.ts",
        "default": "./dist/client.js"
      },
      "require": {
        "types": "./dist/client.d.cts",
        "default": "./dist/client.cjs"
      }
    },
    "./abis": {
      "import": {
        "types": "./dist/abis.d.ts",
        "default": "./dist/abis.js"
      },
      "require": {
        "types": "./dist/abis.d.cts",
        "default": "./dist/abis.cjs"
      }
    }
  },
  "description": "",
  "engines": {
    "node": ">=14",
    "bun": ">=1.0.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "license": "ISC",
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "dev": "bun --watch src/index.ts",
    "build": "tsdown",
    "compile": "tsdown",
    "prepublishOnly": "bun run build",
    "fix": "gts fix",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/package.d.ts"
}
